{
  "expo": {
    "name": "HiIndia",
    "slug": "HiIndia_App",
    "scheme": "hiindiaapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "backgroundColor":"#000",
    "newArchEnabled": true,
    "splash": {},
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deep_1012.HiIndia_App"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.deep_1012.HiIndia_App",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "@react-native-google-signin/google-signin"
      ]

    ],
    "extra": {
      "eas": {
        "projectId": "12097d52-a643-46a9-85ec-1b2b469c02df"
      }
    }
  }
}
