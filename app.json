{
  "expo": {
    "name": "HiIndia",
    "slug": "HiIndia_App",
    "scheme": "hiindiaapp",
    "version": "0.0.08",
    "orientation": "portrait",
    "icon": "./assets/icon1.jpg",
    "userInterfaceStyle": "light",
    "backgroundColor": "#000",
    "newArchEnabled": true,
    "splash": {},
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.hiindia.app",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "ASAuthorizationAppleID": {
          "AppleSignIn": true
        },
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "HiIndia needs access to your photo library to allow you to select a profile picture.",
        "NSPhotoLibraryAddUsageDescription": "HiIndia needs access to your photo library to save selected images."
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.hiindia.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon1.jpg",
        "backgroundColor": "#000"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "12097d52-a643-46a9-85ec-1b2b469c02df"
      }
    }
  }
}
