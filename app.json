{
  "expo": {
    "name": "HiIndia",
    "slug": "HiIndia_App",
    "scheme": "hiindiaapp",
    "version": "0.0.05",
    "orientation": "portrait",
    "icon": "./assets/icon1.jpg",
    "userInterfaceStyle": "light",
    "backgroundColor": "#000",
    "newArchEnabled": true,
    "splash": {},
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.hiindia.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ASAuthorizationAppleID": {
          "AppleSignIn": true
        }
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.deep_1012.HiIndia_App",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon1.jpg",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "2f46a03f-7ae4-4b07-85e5-e01810dac880"
      }
    }
  }
}
