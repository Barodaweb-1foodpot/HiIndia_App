{
  "expo": {
    "name": "HiIndia",
    "slug": "HiIndia_App",
    "scheme": "hiindiaapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.jpg",
    "userInterfaceStyle": "light",
    "backgroundColor": "#000",
    "newArchEnabled": true,
    "splash": {},
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.hiindia.app",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSAppleMusicUsageDescription": "Allow notifications to receive updates",
        "NSCameraUsageDescription": "Allow notifications to receive updates",
        "NSMicrophoneUsageDescription": "Allow notifications to receive updates",
        "NSPhotoLibraryUsageDescription": "Allow notifications to receive updates"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.deep_1012.HiIndia_App",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon1.jpg",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      ["@react-native-google-signin/google-signin"],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "12097d52-a643-46a9-85ec-1b2b469c02df"
      }
    }
  }
}
